							<address>
								<addrLine>
								 <placeName ref="TEI-ListPlace.xml#Put">Puteau</placeName> –				<placeName>N:° 19 bis rampe</placeName>
								</addrLine>
								<lb xml:id="LL1.16_lb_1fr_02" n="2" facs="#LL1.16_line_1fr_02"/>
